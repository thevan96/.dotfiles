snippet cl
console.log(${1:variable});
endsnippet

snippet log
console.log(\`ğŸ $1 >> \`, ${1:variable});
endsnippet

snippet logt
console.log(\`ğŸ $1[${$2}] \`, ${1:variable}[${2:index}]);
endsnippet

snippet logtt
console.log(\`ğŸ $1[${$2}][${$3}] \`, ${1:variable}[${2:index}][${3:index}]);
endsnippet

snippet sw
${1:a} = ${2:b} + (${2:b}=${1:a}, 0);
endsnippet

snippet forl
for (let ${1:i} = 0; $1 < ${2:variable}; $1++) {
	${3:Code}
}
endsnippet

snippet dw
document.write(${1:content});
endsnippet

snippet ls
console.log("Load script...");
endsnippet
