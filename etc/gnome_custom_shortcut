command='flameshot gui'
name='Flameshot'

[custom-keybindings/custom1]
binding='<Shift><Super>p'
command='gpick -p'
name='Gpick'

[custom-keybindings/custom2]
binding='<Shift><Super>w'
command='rofi -show window -show-icons'
name='Rofi_window'

[custom-keybindings/custom3]
binding='<Shift><Super>r'
command='rofi -modi run -show run -show-icons'
name='Rofi_run'

[custom-keybindings/custom4]
binding='<Shift><Super>d'
command='rofi -modi drun -show drun -show-icons'
name='Rofi_drun'

[custom-keybindings/custom5]
binding='<Shift><Super>q'
command='wmctrl -c :ACTIVE:'
name='Close_curren_window'

[custom-keybindings/custom6]
binding='<Shift><Super>o'
command='fast_open'
name='Fast_open'

[custom-keybindings/custom7]
binding='<Super>space'
command='ibus engine xkb:us::eng'
name='Ibus_en'

[custom-keybindings/custom8]
