# super + 1
#   jumpapp.sh alacritty
#
# super + 2
#   jumpapp.sh google-chrome
#
# super + 3
#   jumpapp.sh evince
#
# super + 4
#   nautilus
#
# super + 9
#   jumpapp.sh anki
#
# super + 0
#   gnome-pomodoro

super + {1-9, 0}
  wmctrl -s {0-8,9}

super + shift + {1-9, 0}
  wmctrl -r :ACTIVE: -t {0-8, 9}

super + shift + q
  wmctrl -c :ACTIVE:

super + shift + d
  rofi -modi drun -show drun -show-icons

super + shift + r
  rofi -show run

super + shift + w
  rofi -show window

super + shift + s
  flameshot gui

super + shift + p
  gpick -p

super + shift + o
  fast_open.sh
